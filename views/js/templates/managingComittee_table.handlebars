<table class="table table-sm table-hover" id="managingComitteeTable" data-page-length='5'>
    <thead>
    <tr>
        <th>Name</th>
        <th>Adresse</th>
        <th>Stadt</th>
        <th>Email</th>
        <th></th>
    </tr>
    </thead>
    <tbody>
    {{#each managingComittee}}
        <tr>
            <td>{{ name }} {{ vorname }}</td>
            <td>{{ strasse }}</td>
            <td>{{ plz }} {{ ort }}</td>
            <td>{{ email }}</td>
            <td>
                <a href="mailto:{{ email }}" class="btn btn-outline-primary btn-sm">E-Mail</a>
            </td>
        </tr>
    {{/each}}
    </tbody>
</table>
